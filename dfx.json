{
  "version": 1,
  "canisters": {
    "stopwatch_backend": {
      "type": "custom",
      "candid": "src/stopwatch_backend/stopwatch_backend.did",
      "wasm": ".kybra/stopwatch_backend/stopwatch_backend.wasm",
      "build": "python3 -m kybra stopwatch_backend src/stopwatch_backend/src/main.py src/stopwatch_backend/stopwatch_backend.did"
    },
    "stopwatch_frontend": {
      "type": "assets",
      "source": [
        "src/stopwatch_frontend/dist"
      ]
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    }
  },
  "defaults": {
    "build": {
      "output": "out"
    },
    "deploy": {
      "network": "local"
    }
  }
}
